<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="464.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaces.AdminController">
   <children>
      <Label fx:id="titleLabel" layoutX="24.0" layoutY="50.0" prefHeight="29.0" prefWidth="163.0" text="Admin" />
      <MenuButton fx:id="settingsButton" layoutX="374.0" layoutY="14.0" mnemonicParsing="false" text="Settings">
        <items>
        </items>
      </MenuButton>
      <Button fx:id="homeButton" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#Home" text="Home" />
      <TableView fx:id="table" layoutX="24.0" layoutY="79.0" onMouseClicked="#clickItem" prefHeight="309.0" prefWidth="433.0">
        <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="120.0" text="Username" />
          <TableColumn fx:id="forenameColumn" prefWidth="120.0" text="Forename" />
            <TableColumn fx:id="surnameColumn" prefWidth="120.0" text="Surname" />
            <TableColumn fx:id="accessColumn" prefWidth="80.0" text="Access" />
        </columns>
      </TableView>
      <Button fx:id="deleteButton" layoutX="262.0" layoutY="412.0" mnemonicParsing="false" onAction="#Delete" text="Delete Selected" />
      <Button fx:id="verifyButton" layoutX="106.0" layoutY="412.0" mnemonicParsing="false" onAction="#Verify" text="Verify Selected" />
   </children>
</AnchorPane>
